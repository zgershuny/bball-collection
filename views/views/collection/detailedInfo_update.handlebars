<p>update all info about the card</p>


<form action="/detailedInfo_change" method="post">
    {{#card}}
    <input type="hidden" id="cardID" name="cardID" value="{{cardID}}">
    <input type="hidden" id="saleID" name="saleID" value="{{saleID}}">
    <input type="hidden" id="teamID" name="teamID" value="{{teamID}}">
    <input type="hidden" id="playerID" name="playerID" value="{{playerID}}">
    <input type="hidden" id="featureID" name="featureID" value="{{featureID}}">
    <input type="hidden" id="slabID" name="slabID" value="{{slabID}}">
    <input type="hidden" id="effectID" name="effectID" value="{{effectID}}">
    <input type="hidden" id="parallelID" name="parallelID" value="{{parallelID}}">
    <input type="hidden" id="gradingID" name="gradingID" value="{{gradingID}}">
    <input type="hidden" id="seriesID" name="seriesID" value="{{seriesID}}">
    <table>
        <div class="form-group">
            <label for="year">Year</label>
            <input type="number" name="year" id="year" value="{{year}}">
            <input type="hidden" id="yearID" name="yearID" value="{{yearID}}">
        </div>
        <div class="form-group">
            <label for="b_name">Brand</label>
            <input type="text" name="b_name" id="b_name" value="{{b_name}}">
            <input type="hidden" id="brandID" name="brandID" value="{{brandID}}">
        </div>
        <div class="form-group">
            <label for="Series">Series</label>
            <input type="text" name="s_name" id="s_name" value="{{s_name}}">
        </div>
        <div class="form-group">
            <label for="p_fName">First Name</label>
            <input type="text" name="p_fName" id="p_fName" value="{{p_fName}}">
        </div>
        <div class="form-group">
            <label for="p_lName">Last Name</label>
            <input type="text" name="p_lName" id="p_lName" value="{{p_lName}}">
        </div>
        <div class="form-group">
            <label for="location">Location</label>
            <input type="text" name="location" id="location" value="{{location}}">
        </div>
        <div class="form-group">
            <label for="t_name">Team</label>
            <input type="text" name="t_name" id="t_name" value="{{t_name}}">
        </div>
        <div class="form-group">
            <label for="f_name">Features</label>
            <input type="text" name="f_name" id="f_name" value="{{f_name}}">
        </div>
        <div class="form-group">
            <label for="#d">numbered</label>
            <input type="text" name="numbered" id="numbered" value="{{numbered}}">
        </div>
        <div class="form-group">
            <label for="out_of">Out Of</label>
            <input type="text" name="out_of" id="out_of" value="{{out_of}}">
        </div>
        <div class="form-group">
            <label for="p_name">Parallel</label>
            <input type="text" name="p_name" id="p_name" value="{{p_name}}">
        </div>
        <div class="form-group">
            <label for="e_name">Effect</label>
            <input type="text" name="e_name" id="e_name" value="{{e_name}}">
        </div>
        <div class="form-group">
            <label for="g_name">Company</label>
            <input type="text" name="g_name" id="g_name" value="{{g_name}}">
        </div>
        <div class="form-group">
            <label for="slab_number"></label>
            <input type="number" min="0" step="1" name="slab_number" id="slab_number" value="{{slab_number}}">
        </div>
        <div class="form-group">
            <label for="Overall_Grade">Overall Grade</label>
            <input type="number" min="0" step=".5" max="10" name="Overall_Grade" id="Overall_Grade" value="{{Overall_Grade}}">
        </div>
        <div class="form-group">
            <label for="Centering">Centering</label>
            <input type="number" min="0" step=".5" max="10" name="Centering" id="Centering" value="{{Centering}}">
        </div>
        <div class="form-group">
            <label for="Corners">Corners</label>
            <input type="number" min="0" step=".5" max="10" name="Corners" id="Corners" value="{{Corners}}">
        </div>
        <div class="form-group">
            <label for="Edges">Edges</label>
            <input type="number" min="0" step=".5" max="10" name="Edges" id="Edges" value="{{Edges}}">
        </div>
        <div class="form-group">
            <label for="Surface">Surface</label>
            <input type="number" min="0" step=".5" max="10" name="Surface" id="Surface" value="{{Surface}}">
        </div>
        <div class="form-group">
            <label for="Auto">Auto</label>
            <input type="number" min="0" step=".5" max="10" name="Auto" id="Auto" value="{{Auto}}">
        </div>
        <div class="form-group">
            <label for="b_date">Buy Date</label>
            <input type="date" name="b_date" id="b_date" value="{{b_date}}">
        </div>
        <div class="form-group">
            <label for="b_price">Buy Price</label>
            <input type="number" min="0.00" step="any" name="b_price" id="b_price" value="{{b_price}}">
        </div>
        <div class="form-group">
            <label for="b_fees">Buy Fees</label>
            <input type="number" min="0.00" step="any" name="b_fees" id="b_fees" value="{{b_fees}}">
        </div>
        <div class="form-group">
            <label for="b_ship">Buy Ship</label>
            <input type="number" min="0.00" step="any" name="b_ship" id="b_ship" value="{{b_ship}}">
        </div>
        <div class="form-group">
            <label for="s_date">Sell Date</label>
            <input type="date" name="s_date" id="s_date" value="{{s_date}}">
        </div>
        <div class="form-group">
            <label for="s_price">Sell Price</label>
            <input type="number" min="0.00" step="any" name="s_price" id="s_price" value="{{s_price}}">
        </div>
        <div class="form-group">
            <label for="s_fees">Sell Fees</label>
            <input type="number" min="0.00" step="any" name="s_fees" id="s_fees" value="{{s_fees}}">
        </div>
        <div class="form-group">
            <label for="s_ship">Sell Ship</label>
            <input type="number" min="0.00" step="any" name="s_ship" id="s_ship" value="{{s_ship}}">
        </div>
        <div class="form-group">
            <label for="note">Notes</label>
            <input type="text" name="note" id="note" value="{{note}}">
        </div>
    </table>
    <button type="submit">Save Changes</button>
        {{/card}}
</form>
